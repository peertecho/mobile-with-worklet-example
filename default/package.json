{
  "name": "default",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "lint": "expo lint",
    "clean": "ROOT=$(git rev-parse --show-toplevel); watchman watch-del $ROOT; watchman watch-project $ROOT",
    "build:worklet": "./scripts/build-worklet.sh",
    "build:worklet:watch": "nodemon --watch worklet --ext js,cjs --exec npm run build:worklet",
    "start": "npm run build:worklet && expo start -d",
    "ios": "expo run:ios",
    "android": "expo run:android",
    "dev:ios": "concurrently --kill-others npm:build:worklet:watch npm:ios",
    "dev:android": "concurrently --kill-others npm:build:worklet:watch npm:android"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.2",
    "@react-navigation/bottom-tabs": "^7.4.7",
    "@react-navigation/elements": "^2.6.4",
    "@react-navigation/native": "^7.1.17",
    "b4a": "^1.7.3",
    "bare-fs": "^4.4.5",
    "bare-path": "^3.0.0",
    "events": "^3.3.0",
    "expo": "^54.0.10",
    "expo-build-properties": "^1.0.9",
    "expo-constants": "^18.0.9",
    "expo-file-system": "~19.0.15",
    "expo-font": "^14.0.8",
    "expo-haptics": "^15.0.7",
    "expo-image": "^3.0.8",
    "expo-linking": "^8.0.8",
    "expo-router": "^6.0.8",
    "expo-splash-screen": "^31.0.10",
    "expo-status-bar": "^3.0.8",
    "expo-symbols": "^1.0.7",
    "expo-system-ui": "^6.0.7",
    "expo-web-browser": "^15.0.7",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "~0.81.4",
    "react-native-b4a": "^0.0.4",
    "react-native-bare-kit": "^0.9.3",
    "react-native-gesture-handler": "^2.28.0",
    "react-native-reanimated": "^4.1.2",
    "react-native-safe-area-context": "^5.6.1",
    "react-native-screens": "^4.16.0",
    "react-native-web": "^0.21.1",
    "react-native-worklets": "^0.6.0",
    "ready-resource": "^1.2.0"
  },
  "devDependencies": {
    "@types/b4a": "^1.6.5",
    "@types/react": "^19.1.15",
    "@types/streamx": "^2.9.5",
    "babel-plugin-react-compiler": "^19.1.0-rc.3",
    "babel-preset-expo": "^54.0.3",
    "bare-pack": "^1.4.6",
    "concurrently": "^9.2.1",
    "eslint": "^9.36.0",
    "eslint-config-expo": "^10.0.0",
    "nodemon": "^3.1.10",
    "typescript": "^5.9.2"
  },
  "imports": {
    "fs": {
      "bare": "bare-fs",
      "default": "fs"
    },
    "path": {
      "bare": "bare-path",
      "default": "path"
    }
  }
}
